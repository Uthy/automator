{
  "name": "The Toolkit",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "A Chrome Extension starter shell",
  "devtools_page": "devtools.html",
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "assets/16x16.png",
    "24": "assets/24x24.png",
